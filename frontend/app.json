{
  "expo": {
    "name": "Gyan Sultanat",
    "slug": "mugaddas-network",
    "version": "1.0.1",
    "description": "Gyaan se Aay, Apne Sapne Sajaye! - India's #1 Education & Earning Platform by Muqaddas Technology",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gyansultanat",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gyansultanat.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Camera for profile photos and content",
        "NSPhotoLibraryUsageDescription": "Access photos for sharing content",
        "NSMicrophoneUsageDescription": "Microphone for voice messages"
      }
    },
    "android": {
      "package": "com.gyansultanat.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1a1a2e"
      },
      "versionCode": 2,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.VIBRATE",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "gyansultanat"
            },
            {
              "scheme": "https",
              "host": "*.muqaddasnetwork.com",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "59db773b-0bd4-4c7c-bc04-e2a361f880ad"
      }
    },
    "owner": "muqaddasnetworks-organization",
    "runtimeVersion": "1.0.1",
    "updates": {
      "url": "https://u.expo.dev/59db773b-0bd4-4c7c-bc04-e2a361f880ad"
    }
  }
}
